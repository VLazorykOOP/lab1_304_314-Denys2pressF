#include <iostream>
using namespace std;

int main()
{
    int a,b,c,d;
    cin>>a>>b>>c>>d;
    int first=(b<<4)+b;
    int second=(a<<11)+(a<<9)+(a<<8)+(a<<7)+(a<<6)+(a<<2);
    int third=(c<<6)+(c<<5)+(c<<4)+(c<<3);
    int fourth=(d<<7)-d;
    double result=((first+second)>>5)-third+fourth;
    cout<<result;
    return 0;
}
